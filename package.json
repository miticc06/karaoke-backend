{
	"name": "nest-typescript-starter",
	"version": "1.0.0",
	"description": "Nest TypeScript starter repository",
	"license": "MIT",
	"scripts": {
		"start:normal": "ts-node src/main",
		"start": "node dist/server",
		"dev": "webpack --config webpack.config.js --watch",
		"build": "webpack --config webpack.config.js",
		"gen": "ts-node src/generate-typings",
		"eslint:check": "tslint -p tsconfig.json -c tslint.json",
		"eslint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
		"husky": {
			"hooks": {
				"pre-commit": "npm run eslint:fix && git add ."
			}
		}
	},
	"dependencies": {
		"@nestjs/common": "6.8.2",
		"@nestjs/core": "6.8.2",
		"@nestjs/graphql": "^6.5.3",
		"@nestjs/platform-express": "6.8.2",
		"@nestjs/typeorm": "^6.2.0",
		"apollo-server-express": "^2.9.5",
		"dotenv": "^8.1.0",
		"graphql": "^14.5.8",
		"graphql-tools": "^4.0.5",
		"mongodb": "^3.3.2",
		"reflect-metadata": "^0.1.13",
		"rxjs": "6.5.3",
		"tslint-plugin-prettier": "^2.0.1",
		"typeorm": "^0.2.19",
		"typescript": "^3.6.3"
	},
	"devDependencies": {
		"@types/node": "12.7.11",
		"husky": "^3.0.8",
		"prettier": "^1.18.2",
		"ts-loader": "6.2.0",
		"ts-node": "8.4.1",
		"tslint": "^5.20.0",
		"tslint-config-prettier": "^1.18.0",
		"webpack": "4.41.0",
		"webpack-cli": "3.3.9",
		"webpack-node-externals": "1.7.2"
	}
}
